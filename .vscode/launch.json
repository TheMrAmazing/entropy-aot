{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/entropy-db/index",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ],
            "type": "node",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--loader",
                "ts-node/esm",
                "--experimental-specifier-resolution=node",
                "-r",
                "tsconfig-paths/register"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}